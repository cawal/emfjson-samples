@GenModel(
    bundleManifest="false",
    modelDirectory="emfjson-jersey-sample/src/main/java-gen",
    complianceLevel="7.0"
)
package sample.jersey.model

class User {
    String name
    Date birthDate
    contains Address[] addresses
    refers User[] friends
}

class Address {
    String street
    String country
    String city
    int number
}
